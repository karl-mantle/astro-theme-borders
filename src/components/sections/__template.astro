---
import type { SectionProps } from '~/types';
import { THEME_CONTAINER_DESKTOP, THEME_CONTAINER_MOBILE } from '~/consts';
import { resolveBackgroundColor } from '~/utils';

interface Props extends SectionProps {
  example: string
};

const { backgroundColor = 'transparent', class: className, ...rest }: Partial<Props> = Astro.props;
---

<section class:list={['flex flex-col mb-8', resolveBackgroundColor(backgroundColor), className]} {...rest} >

</section>
